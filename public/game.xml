<row id='game' width= 120 height= 170 justifyContent='JUSTIFY_CENTER' alignItems='ALIGN_FLEX_START' flexDirection='FLEX_DIRECTION_COLUMN_REVERSE' style='asset: "Background"'>
    <row width= 120 height= 20 justifyContent='JUSTIFY_CENTER' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_ROW'>
        <col width= 60 height= 20 justifyContent='JUSTIFY_FLEX_START' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_ROW'>
            <Text width= 17 height= 13 options='{textHeight: 0.05, textAnchor: "middle-left"}' style='textColor: "#F54545"'>Game Mode</Text>
        </col>
        <col width= 60 height= 20 justifyContent='JUSTIFY_FLEX_END' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_ROW'>
            <Text width= 13 height= 13 options='{textHeight: 0.05, textAnchor: "middle-right"}' style='textColor: "#F54545"'>Controls</Text>
        </col>
    </row>
    <row width= 120 height= 150 justifyContent='JUSTIFY_FLEX_START' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_COLUMN_REVERSE'>
        <row width= 120 height= 25 justifyContent='JUSTIFY_CENTER' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_ROW'>
            <col width= 60 height= 20 justifyContent='JUSTIFY_FLEX_START' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_ROW'>
                <Button class='start' id='target_practice_start' margins= 4 width= 6 height= 6 style='asset: "Play"'></Button>
                <Text width= 40 height= 15 options='{textHeight: 0.05, textAnchor: "middle-center"}' style='asset: "Attachment Cell"'>Target Practice</Text>
            </col>
            <col width= 50 height= 20 justifyContent='JUSTIFY_FLEX_END' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_ROW'>
                <col width= 20 height= 15 marginRight=1 justifyContent='JUSTIFY_CENTER' alignItems='ALIGN_FLEX_END' flexDirection='FLEX_DIRECTION_COLUMN_REVERSE'>
                    <Text width= 1 height= 5 marginRight= -1 options='{textHeight: 0.025, textAnchor: "middle-right"}'>respawn</Text>
                    <Checkbox id='respawn_checkbox' marginTop= 1 width= 12 height= 5 options='{checked: "Switch On", unchecked: "Switch Off"}'></Checkbox>
                </col>
            </col>
        </row>
        <row width= 120 height= 25 justifyContent='JUSTIFY_CENTER' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_ROW'>
            <col width= 60 height= 20 justifyContent='JUSTIFY_FLEX_START' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_ROW'>
                <Button class='start' id='whackamole_start' margins= 4 width= 6 height= 6 style='asset: "Play"'></Button>
                <Text width= 40 height= 15 options='{textHeight: 0.05, textAnchor: "middle-center"}' style='asset: "Attachment Cell"'>Whackamole</Text>
            </col>
            <col width= 50 height= 20 justifyContent='JUSTIFY_FLEX_END' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_ROW'>
                <col width= 30 height= 15 marginRight=1 justifyContent='JUSTIFY_CENTER' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_COLUMN_REVERSE'>
                    <Text width= 1 height= 5 marginRight= -1 options='{textHeight: 0.025, textAnchor: "middle-center"}'>moving</Text>
                    <Checkbox id='moving_checkbox' marginTop= 1 width= 12 height= 5 options='{checked: "Switch On", unchecked: "Switch Off"}'></Checkbox>
                </col>
                <col width= 20 height= 15 marginRight= 5 justifyContent='JUSTIFY_CENTER' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_COLUMN_REVERSE'>
                    <Text width= 5 height= 5 options='{textHeight: 0.025, textAnchor: "middle-center"}'>timer</Text>
                    <Number id='whackamole_timer' height= 7 justifyContent='JUSTIFY_CENTER' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_ROW' style='asset: "Enemy Cell";'>
                        <Button id='num' width= 7 height= 7 margins= 2 options='{textHeight: 0.04, textAnchor: "middle-center"}' style='textFont: "monospace";'>69</Button>
                        <Button id='dec' width= 4 height= 4 margins= 1 style='asset: "Minus";'></Button>
                        <Button id='inc' width= 4 height= 4 margins= 1 style='asset: "Plus";'></Button>
                    </Number>
                </col>
            </col>
        </row>
        <row width= 120 height= 25 justifyContent='JUSTIFY_CENTER' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_ROW'>
            <col width= 60 height= 20 justifyContent='JUSTIFY_FLEX_START' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_ROW'>
                <Button class='start' id='search_and_destroy_start' margins= 4 width= 6 height= 6 style='asset: "Play"'></Button>
                <Text width= 40 height= 15 options='{textHeight: 0.035, textAnchor: "middle-center"}' style='asset: "Attachment Cell"'>Search and Destroy</Text>
            </col>
            <col width= 50 height= 20 justifyContent='JUSTIFY_FLEX_END' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_ROW'>
                <col width= 20 height= 15 marginRight= 5 justifyContent='JUSTIFY_CENTER' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_COLUMN_REVERSE'>
                    <Text width= 5 height= 5 options='{textHeight: 0.025, textAnchor: "middle-center"}'>count</Text>
                    <Number id='search_and_destroy_number' height= 7 justifyContent='JUSTIFY_CENTER' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_ROW' style='asset: "Enemy Cell";'>
                        <Button id='num' width= 7 height= 7 margins= 2 options='{textHeight: 0.04, textAnchor: "middle-center"}' style='textFont: "monospace";'>69</Button>
                        <Button id='dec' width= 4 height= 4 margins= 1 style='asset: "Minus";'></Button>
                        <Button id='inc' width= 4 height= 4 margins= 1 style='asset: "Plus";'></Button>
                    </Number>
                </col>
            </col>
        </row>
        <row width= 120 height= 25 justifyContent='JUSTIFY_CENTER' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_ROW'>
            <col width= 60 height= 20 justifyContent='JUSTIFY_FLEX_START' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_ROW'>
                <Button class='start' id='zombie_horde_start' margins= 4 width= 6 height= 6 style='asset: "Play"'></Button>
                <Text width= 40 height= 15 options='{textHeight: 0.05, textAnchor: "middle-center"}' style='asset: "Attachment Cell"'>Zombie Horde</Text>
            </col>
            <col width= 50 height= 20 justifyContent='JUSTIFY_FLEX_END' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_ROW'>
                <col width= 20 height= 15 marginRight= 5 justifyContent='JUSTIFY_CENTER' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_COLUMN_REVERSE'>
                    <Text width= 5 height= 5 options='{textHeight: 0.025, textAnchor: "middle-center"}'>speed</Text>
                    <Button id='zombie speed' width= 15 height= 7 margins= 1 style='asset: "Enemy Cell";'>Normal</Button>
                </col>
                <col width= 20 height= 15 marginRight= 5 justifyContent='JUSTIFY_CENTER' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_COLUMN_REVERSE'>
                    <Text width= 5 height= 5 options='{textHeight: 0.025, textAnchor: "middle-center"}'>count</Text>
                    <Number id='zombie_horde_number' height= 7 justifyContent='JUSTIFY_CENTER' alignItems='ALIGN_CENTER' flexDirection='FLEX_DIRECTION_ROW' style='asset: "Enemy Cell";'>
                        <Button id='num' width= 7 height= 7 margins= 2 options='{textHeight: 0.04, textAnchor: "middle-center"}' style='textFont: "monospace";'>69</Button>
                        <Button id='dec' width= 4 height= 4 margins= 1 style='asset: "Minus";'></Button>
                        <Button id='inc' width= 4 height= 4 margins= 1 style='asset: "Plus";'></Button>
                    </Number>
                </col>
            </col>
        </row>
    </row>
</row>